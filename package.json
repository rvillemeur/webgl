    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "jsdom": "^13.2.0",
    "mocha": "^5.2.0",
    "standard": "^12.0.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
